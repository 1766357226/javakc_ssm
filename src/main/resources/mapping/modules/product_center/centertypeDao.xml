<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhg.javakc.modules.product_center.products_type.dao.CenterTypeDao">
    <select id="queryType" resultType="java.util.HashMap">
        SELECT
        TYPE_ID "id",
        TYPE_NAME "name",
        TYPE_NUM "orglevel",
        TYPE_PARENTID "pId",
        DECODE(TYPE_OPEN,0,'false',1,'true') "open"
        FROM  GOODS_TYPE WHERE  1=1
    </select>
    <insert id="insert" >
        INSERT INTO GOODS_TYPE (TYPE_ID,TYPE_NAME,TYPE_NUM,TYPE_PARENTID,TYPE_OPEN) VALUES
        (#{ typeId},#{typeName},#{typeNum},#{typeParentId},#{typeOpen})
    </insert>
</mapper>